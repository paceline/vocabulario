<% if taglist_detail.blank? %>
  <em class="tag"><%= link_to_function("This vocabulary is currently untagged.", "$('taglist','tag_form').invoke('toggle')", :class => "edit") %></em>
<% else %>
  <% taglist_detail.each do |tag| %>
     <% if logged_in? %>
       <em class="tag"><%= link_to_function(tag, "$('taglist','tag_form').invoke('toggle')", :class => "edit") %></em>
     <% else %>
       <em class="tag"><%= tag %></em>
     <% end %>
   <% end %>
<% end %>