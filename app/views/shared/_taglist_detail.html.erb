<% if taglist_detail.tag_list.empty? %>
  <em class="tag">This vocabulary is currently untagged.</em>
<% else %>
  <% taglist_detail.tags.each do |tag| %>
    <% if logged_in? %>
      <em class="tag"><%= link_to_function(tag.name, "$('taglist','tag_form').invoke('toggle')", :class => "edit") %></em>
    <% else %>
      <em class="tag"><%= tag.name %></em>
    <% end %>
  <% end %>
<% end %>